 {{> nav home=home }} {{> jumbo }}
<div class="container-fluid articles">
    {{#each articles}}
    <div data-_id={{_id}} class="card">
        <div class="card-header">
            <h3>
                <a class="article-link" target="_blank" rel="noopener noreferrer" href={{link}}>{{title}}</a>
                <a class="btn btn-success delete">Delete From Saved</a>
                <a class="btn btn-success notes">Article Notes</a>
            </h3>
        </div>
        <div class="card-body">{{summary}}</div>
    </div>
    {{/each}}

</div>
